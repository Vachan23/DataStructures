    "def gradingStudents(grades):\n",
    "    for i in range(len(grades)):\n",
    "        if grades[i] < 38: continue\n",
    "        diff = grades[i] % 5\n",
    "        if 5 - diff < 3:\n",
    "            grades[i] += 5 - diff\n",
    "    return grades"
